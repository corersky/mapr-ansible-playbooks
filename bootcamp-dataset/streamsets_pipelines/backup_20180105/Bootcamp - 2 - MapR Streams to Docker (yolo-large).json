      "instanceName" : "Shell_01",
      "library" : "streamsets-datacollector-basic-lib",
      "stageName" : "com_streamsets_pipeline_stage_executor_shell_ShellDExecutor",
      "stageVersion" : "1",
      "configuration" : [ {
        "name" : "config.environmentVariables",
        "value" : [ {
          "key" : "IMAGE_FILENAME",
          "value" : "${record:value('/fileInfo/filename')}"
        } ]
      }, {
        "name" : "config.timeout",
        "value" : "1000"
      }, {
        "name" : "config.script",
        "value" : "if [ \"$(sudo docker ps|grep mapr-pacc-image-classification|wc -l)\" -eq 1 ]; then\n  \techo \"$(date) - Docker Image Classification already running\" >> /tmp/sdc_docker.txt\nelse\n  \techo \"$(date) - Docker Image Classification not running, starting it now\" >> /tmp/sdc_docker.txt\n\n    # Get the MapR Cluster name\n    MAPR_CLUSTER_NAME=`awk '{print $1}' /opt/mapr/conf/mapr-clusters.conf`\n\n    # Get the CLDB hostname\n    MAPR_CLDB_HOST_NAME=`awk '{print $3}' /opt/mapr/conf/mapr-clusters.conf`\n    MAPR_CLDB_HOST_NAME=${MAPR_CLDB_HOST_NAME%:7222}\n\n    # Get the mapr uid and gid\n    MAPR_UID=`id -u mapr`\n    MAPR_GID=`id -g mapr`\n\n    # File paths\n    IMAGE_INPUT_PATH=/mapr/$MAPR_CLUSTER_NAME/image-classification/input/\n    IMAGE_OUTPUT_PATH=/mapr/$MAPR_CLUSTER_NAME/image-classification/input/\n\n    echo \"Starting classification for image: $IMAGE_INPUT_PATH$IMAGE_FILENAME\" >> /tmp/sdc_docker.txt\n    \n    # Run the Docker container to classify the image\nflock -n /tmp/sdc_docker.lock -c \"\\\n\tsudo docker run -i \\\n\t--cap-add SYS_ADMIN \\\n\t--cap-add SYS_RESOURCE \\\n\t--device /dev/fuse \\\n\t-e MAPR_CLUSTER=$MAPR_CLUSTER_NAME \\\n\t-e MAPR_CLDB_HOSTS=$MAPR_CLDB_HOST_NAME \\\n\t-e MAPR_CONTAINER_USER=mapr \\\n\t-e MAPR_CONTAINER_GROUP=mapr \\\n\t-e MAPR_CONTAINER_UID=$MAPR_UID \\\n\t-e MAPR_CONTAINER_GID=$MAPR_GID \\\n\t-e MAPR_MOUNT_PATH=/mapr \\\n\t-e YOLO_IMAGE_FILENAME=$IMAGE_FILENAME \\\n\t-e YOLO_INPUT=/mapr/$MAPR_CLUSTER_NAME/image-classification/input/ \\\n\t-e YOLO_OUTPUT=/mapr/$MAPR_CLUSTER_NAME/image-classification/output/ \\\n\t-e YOLO_CONFIG=/mapr/$MAPR_CLUSTER_NAME/yolo/yolo.cfg \\\n\t-e YOLO_WEIGHTS=/mapr/$MAPR_CLUSTER_NAME/yolo/yolo.weights \\\n\t-e YOLO_THRESHOLD=0.1 \\\n\tmkieboom/mapr-pacc-image-classification >> /tmp/sdc_docker.txt &\"\nfi\n\ndocker rm $(docker ps -q -f status=exited) &"
